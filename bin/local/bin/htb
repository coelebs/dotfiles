#!/usr/bin/env python3
import sys

byte = int(sys.argv[1], 16)
bits = bin(byte)[2:][::-1]

enabled = list()
idx = 0
for c in bits:
    print(f'{idx}: {c}')
    if c == '1':
        enabled.append(str(idx))

    idx += 1

print("\nEnabled pins:")
print(" ".join(enabled))

print("Decimal value: ", byte)
